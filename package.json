{
  "name": "angulartics-localforage",
  "version": "0.0.1",
  "description": "Localforage plugin for Angulartics",
  "keywords": [
    "local",
    "forage",
    "angular",
    "angulartics"
  ],
  "author": "Justin Maslin <me@justinmaslin.com> (https://github.com/jmaslin)",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "mkdir -p dist && uglifyjs lib/angulartics-localforage.js --compress --source-map dist/angulartics-localforage.min.js.map --source-map-root .. --source-map-url ../dist/angulartics-localforage.min.js.map -o dist/angulartics-localforage.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmaslin/angulartics-localforage"
  },
  "bugs": {
    "url": "https://github.com/jmaslin/angulartics-localforage/issues"
  },
  "license": "MIT",
  "peerDependencies": {
    "angulartics": "~1.0.0",
    "angular-localforage": "~1.2.5"
  },
  "devDependencies": {
    "uglify-js": "^2.4.23"
  }
}
